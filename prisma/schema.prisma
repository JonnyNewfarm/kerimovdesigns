generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  title String

  src       String   @db.String
  src2      String?  @db.String
  src3      String?  @db.String
  src4      String?  @db.String
  src5      String?  @db.String
  src6      String?  @db.String
  src7      String?  @db.String
  src8      String?  @db.String
  src9      String?  @db.String
  srcVideo  String?  @db.String
  role      String?
  type      String?
  tools     String?
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}

model User {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  email    String  @unique
  password String
  name     String?
}
